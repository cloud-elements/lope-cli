{
  "bin": {"lope": "index.js"},
  "dependencies": {
    "execa": "0.6.x",
    "lope": "0.1.x",
    "meow": "3.x",
    "minimist": "1.x",
    "ramda": "0.23.x",
    "sanctuary": "0.12.x"
  },
  "description": "Transform NPM package scripts into simple CLIs",
  "devDependencies": {
    "ava": "0.18.x",
    "lope-example": "0.0.x",
    "npm-check": "5.x",
    "nyc": "10.x",
    "xo": "0.17.x"
  },
  "engines": {"node": "^7.6.0"},
  "homepage": "https://github.com/cloud-elements/lope-cli",
  "keywords": [
    "cli",
    "npm",
    "package",
    "packages",
    "run",
    "script",
    "scripts"
  ],
  "license": "ISC",
  "name": "lope-cli",
  "nyc": {
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "statements": 100
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cloud-elements/lope-cli.git"
  },
  "scripts": {
    "clean": "rm -rf .nyc_output && rm -rf coverage && rm -rf node_modules",
    "cover": "nyc report --reporter=text-lcov",
    "coverage": "nyc report --reporter=lcov && open ./coverage/lcov-report/index.html",
    "depend": "npm-check --ignore lope-example",
    "echo": "echo ${npm_package_config_echo}",
    "false": "false",
    "lint": "xo",
    "test": "{ npm run depend || true; } && npm run lint && npm run unit",
    "true": "true",
    "unit": "npm install --global lope-example && nyc --check-coverage ava && npm uninstall --global lope-example"
  },
  "version": "0.1.0",
  "xo": {"esnext": true}
}
